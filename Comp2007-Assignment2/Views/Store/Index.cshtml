@using Comp2007_Assignment2.Models
@model IEnumerable<Comp2007_Assignment2.Models.Type>

@{
    ViewBag.Title = "Index";

}

                      <h3>Browse Types</h3>
                      <p>
                          Select from @Model.Count()
                          types:
                      </p>
                      <ul>
                          @foreach (Comp2007_Assignment2.Models.Type item in Model)
                          {

                          <li>@Html.ActionLink(item.Name,"Browse", new {types=item.Name})</li>
                          }
                      </ul>
